@model DataSeedVM
<div class="container my-2">
    @if (Model.IsError)
    {
        <div class="alert alert-danger" role="alert">
            <h3 class="text-center">@Model.Description</h3>
        </div>
    }
    else
    {
        <div class="alert alert-success" role="alert">
            <h3 class="text-center">@Model.Description</h3>  
        </div>
    }

</div>

